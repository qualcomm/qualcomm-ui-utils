{
  "compilerOptions": {
    "esModuleInterop": true,
    "composite": true,
    "forceConsistentCasingInFileNames": true,
    "strict": false,
    "strictPropertyInitialization": false,
    "jsx": "react-jsx",
    "module": "ESNext",
    "moduleResolution": "bundler",
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
    "target": "ESNext",
    "verbatimModuleSyntax": false
  },
  "include": ["./scripts"],
}
